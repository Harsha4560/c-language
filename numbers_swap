#include<stdio.h>
void main()
{
  int a=10, b=5;          // initializing the two variables a and b
  printf("Initial values of a and b are a = %d b = %d\n", a, b);  //displaying the initial values of a and b
  a = a + b;            //swapping the variables a and b
  b = a - b;
  a = a - b;
  printf("Final values of a and b are a = %d b = %d\n", a, b);       //displaying the swapped values of a and b
}
